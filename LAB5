  2    rest VARCHAR2(15);
  3    locate VARCHAR2(15);
  4    lastname VARCHAR2(10);
  5  BEGIN
  6    empinfo('minoso', rest, locate);
  7    DBMS_OUTPUT.PUT_LINE('dines at '||rest||
  8  	 ' which is located in '||locate);
  9  END;
 10  .
